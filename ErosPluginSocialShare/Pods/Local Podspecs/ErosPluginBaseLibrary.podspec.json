{
  "name": "ErosPluginBaseLibrary",
  "version": "1.3.5",
  "summary": "WeexEros iOS App base library",
  "description": "WeexEros iOS App base library",
  "homepage": "https://github.com/bmfe/eros-plugin-ios-baseLibrary",
  "license": "MIT",
  "authors": {
    "xionghuayu": "18601949015@163.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/bmfe/eros-plugin-ios-baseLibrary.git",
    "tag": "1.3.5"
  },
  "requires_arc": true,
  "dependencies": {
    "YYText": [
      "1.0.7"
    ],
    "Masonry": [
      "1.1.0"
    ],
    "YYModel": [
      "1.0.4"
    ],
    "SSZipArchive": [
      "1.6.2"
    ],
    "YTKNetwork": [
      "2.0.3"
    ],
    "SocketRocket": [
      "0.4.2"
    ],
    "SDWebImage": [
      "3.7.6"
    ],
    "MJRefresh": [
      "3.1.12"
    ],
    "CTMediator": [
      "13"
    ]
  },
  "subspecs": [
    {
      "name": "BMDevice",
      "source_files": "Source/BMDevice/*.{c,h,m,mm,S}",
      "frameworks": "Security",
      "requires_arc": false
    },
    {
      "name": "BMTransition",
      "source_files": "Source/BMTransition/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMTransition/*.h",
      "requires_arc": true
    },
    {
      "name": "BMDebug",
      "source_files": "Source/BMDebug/**/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMDebug/**/*.h",
      "requires_arc": true
    },
    {
      "name": "BMExtension",
      "source_files": "Source/BMExtension/**/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMExtension/**/*.h",
      "requires_arc": true,
      "libraries": "bz2"
    },
    {
      "name": "BMWeexExtension",
      "source_files": "Source/BMWeexExtension/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMWeexExtension/*.h",
      "resources": "Source/BMWeexExtension/Resources/*",
      "requires_arc": true
    },
    {
      "name": "BMController",
      "source_files": "Source/BMController/**/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMController/**/*.h",
      "requires_arc": true
    },
    {
      "name": "BMNetwork",
      "source_files": "Source/BMNetwork/**/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMNetwork/**/*.h",
      "requires_arc": true
    },
    {
      "name": "BMModule",
      "source_files": "Source/BMModule/**/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMModule/**/*.h",
      "resources": "Source/BMModule/Modal/SVProgressHUD/SVProgressHUD.bundle",
      "requires_arc": true,
      "dependencies": {
        "Realm": [
          "3.1.0"
        ],
        "BindingX": [
          "1.0.3"
        ],
        "TZImagePickerController": [
          "3.1.2"
        ]
      }
    },
    {
      "name": "BMManager",
      "source_files": "Source/BMManager/**/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMManager/**/*.h",
      "requires_arc": true
    },
    {
      "name": "BMHandler",
      "source_files": "Source/BMHandler/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMHandler/*.h",
      "requires_arc": true
    },
    {
      "name": "BMCustomUI",
      "source_files": "Source/BMCustomUI/**/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMCustomUI/**/*.h",
      "requires_arc": true
    },
    {
      "name": "BMComponent",
      "source_files": "Source/BMComponent/**/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMComponent/**/*.h",
      "requires_arc": true,
      "resources": [
        "Source/BMComponent/Calendar/Resources/*.png",
        "Source/BMComponent/Chart/Resources/*"
      ],
      "dependencies": {
        "FSCalendar": [
          "2.7.8"
        ],
        "YYText": [
          "1.0.7"
        ]
      }
    },
    {
      "name": "BMModel",
      "source_files": "Source/BMModel/*.{c,h,m,mm,S}",
      "public_header_files": "Source/BMModel/*.h",
      "requires_arc": true
    },
    {
      "name": "ErosApp",
      "source_files": "Source/ErosApp/**/*.{c,h,m,mm,S,pch}",
      "public_header_files": "Source/ErosApp/**/*.h",
      "requires_arc": true,
      "prefix_header_file": "Source/ErosApp/ErosDefine/PrefixHeader.pch"
    }
  ]
}
